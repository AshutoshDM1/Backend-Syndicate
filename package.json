    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "dependencies": {
    "@prisma/client": "6.9.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.2.8",
    "cors": "^2.8.5",
