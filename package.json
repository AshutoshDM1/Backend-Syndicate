{
  "name": "backend-syndicate",
  "version": "0.0.1",
  "description": "Backend Server of Restaurant POS System",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AshutoshDM1/Backend-Syndicate.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
